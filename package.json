{
  "name": "llm-interop",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "packageManager": "bun@1.2.20",
  "sideEffects": false,
  "scripts": {
    "test": "vitest run",
    "test:unit": "vitest run --exclude='**/*.{network,integration}.{spec,test}.{ts,tsx}' --reporter=basic --poolOptions.threads.maxThreads=2",
    "test:integration": "vitest run 'src/**/*.integration.{spec,test}.{ts,tsx}'",
    "test:network": "vitest run 'src/**/*.network.{spec,test}.{ts,tsx}'",
    "test:all": "vitest run --exclude='**/node_modules/**'",
    "test:cov": "vitest run --coverage",
    "test:cov:unit": "vitest run --coverage --exclude='**/*.{network,integration}.{spec,test}.{ts,tsx}'",
    "build": "vite build",
    "clean": "rimraf dist",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "format": "prettier --write .",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./fetch": {
      "types": "./dist/ports/fetch/index.d.ts",
      "import": "./dist/fetch.js",
      "require": "./dist/fetch.cjs"
    },
    "./fetch/openai": {
      "types": "./dist/ports/fetch/openai.d.ts",
      "import": "./dist/fetch/openai.js",
      "require": "./dist/fetch/openai.cjs"
    },
    "./fetch/gemini": {
      "types": "./dist/ports/fetch/gemini.d.ts",
      "import": "./dist/fetch/gemini.js",
      "require": "./dist/fetch/gemini.cjs"
    },
    "./fetch/claude": {
      "types": "./dist/ports/fetch/claude.d.ts",
      "import": "./dist/fetch/claude.js",
      "require": "./dist/fetch/claude.cjs"
    },
    "./adapters/openai-compatible": {
      "types": "./dist/adapters/openai-compatible/index.d.ts",
      "import": "./dist/adapters/openai-compatible/index.js",
      "require": "./dist/adapters/openai-compatible/index.cjs"
    },
    "./adapters/openai-to-claude": {
      "types": "./dist/adapters/openai-to-claude/index.d.ts",
      "import": "./dist/adapters/openai-to-claude/index.js",
      "require": "./dist/adapters/openai-to-claude/index.cjs"
    },
    "./adapters/gemini-to-openai": {
      "types": "./dist/adapters/gemini-to-openai/index.d.ts",
      "import": "./dist/adapters/gemini-to-openai/index.js",
      "require": "./dist/adapters/gemini-to-openai/index.cjs"
    },
    "./adapters/openai-to-gemini-v1beta": {
      "types": "./dist/adapters/openai-to-gemini-v1beta/index.d.ts",
      "import": "./dist/adapters/openai-to-gemini-v1beta/index.js",
      "require": "./dist/adapters/openai-to-gemini-v1beta/index.cjs"
    },
    "./adapters/claude-to-openai": {
      "types": "./dist/adapters/claude-to-openai/index.d.ts",
      "import": "./dist/adapters/claude-to-openai/index.js",
      "require": "./dist/adapters/claude-to-openai/index.cjs"
    },
    "./providers/openai": {
      "types": "./dist/providers/openai/index.d.ts",
      "import": "./dist/providers/openai/index.js",
      "require": "./dist/providers/openai/index.cjs"
    },
    "./providers/claude": {
      "types": "./dist/providers/claude/index.d.ts",
      "import": "./dist/providers/claude/index.js",
      "require": "./dist/providers/claude/index.cjs"
    },
    "./providers/gemini": {
      "types": "./dist/providers/gemini/index.d.ts",
      "import": "./dist/providers/gemini/index.js",
      "require": "./dist/providers/gemini/index.cjs"
    }
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.11",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitest/coverage-v8": "^3.2.4",
    "ajv": "^8.12.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-formatter-compact": "^8.40.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^54.1.1",
    "prettier": "^3.6.2",
    "@anthropic-ai/sdk": "^0.60.0",
    "google-auth-library": "^9.14.1",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "vite": "^7.1.3",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@google/genai": "^1.16.0",
    "openai": "^5.15.0",
    "tiktoken": "^1.0.22"
  }
}
